<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Jhkinfotech_Ticket::ticket_content" title="Jhkinfotech Ticket Menu"  sortOrder="10">
                    <resource id="Jhkinfotech_Ticket::ticket_title" title="Support Ticket"  sortOrder="10">
                        <resource id="Jhkinfotech_Ticket::viewticket" title="View Tickets"  sortOrder="20" />
                        <resource id="Jhkinfotech_Ticket::post" title="Tickets Post"  sortOrder="30" />
                        <resource id="Jhkinfotech_Ticket::department" title="Department"  sortOrder="40" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Jhkinfotech_Ticket::settings" title="Ticket Settings"/>
                        </resource>
                    </resource>
                </resource>
            </resource>            
        </resources>
    </acl>
</config>
