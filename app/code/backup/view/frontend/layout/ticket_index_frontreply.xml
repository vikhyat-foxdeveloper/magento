<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd"> 
<head>
    <title>My Support Tickets</title>
</head>
<update handle="customer_account"/> 
    <body>        
        <referenceContainer name="content">
            <block class="Jhkinfotech\Ticket\Block\Frontreply" name="ticket_customer_frontreply" template="Jhkinfotech_Ticket::Index/frontreply.phtml" cacheable="false">            
            </block> 
        </referenceContainer>
        <referenceBlock name="support-ticket">
                <arguments>
                    <argument name="current" xsi:type="boolean">true</argument>
                </arguments>
        </referenceBlock>
    </body> 
</page>
